{
  "manifest_version": 2,
  "name": "Quick Clipboard",
  "description": "Now u can copy code quicker by presing copy button or just right click. ",
  "version": "1.0.0",
  "icons": {
    "16": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  },
  "content_scripts": [{
    "js": ["content_script.js"],
    "css": ["style.css"],
    "matches": ["<all_urls>"],
    "run_at": "document_end"
  }],
  "browser_action": {
    "default_popup": "popup.html",
    "default_icon": "icon.png"
  }
}